AT_SETUP([mu-message-delete])
AT_KEYWORDS([message])
MU_GUILE_CHECK([
(let ((msg (test-message)))
  (display msg)(newline)
  (mu-message-delete msg)
  (display msg)(newline)
  (mu-message-delete msg #f)
  (display msg)(newline))],
 0,
[#<message "user@example.org" "Fri Jun  8 14:30" 10 539>
#<message "user@example.org" "Fri Jun  8 14:30" 10 539 D>
#<message "user@example.org" "Fri Jun  8 14:30" 10 539>
])
AT_CLEANUP
