AT_SETUP([mu-body?])
AT_KEYWORDS([body])
MU_GUILE_CHECK([
(let ((msg (mu-message-create)))
  (if (mu-body? msg) (exit 1))
  (if (mu-body? (mu-message-get-body msg)) (exit 0))
  (exit 2))])
AT_CLEANUP
