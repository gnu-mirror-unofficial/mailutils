AT_SETUP([Constants])
AT_KEYWORDS([constants])
AT_DATA([expout],
[AT_PACKAGE_TARNAME
AT_PACKAGE_VERSION
AT_PACKAGE_STRING
AT_PACKAGE_BUGREPORT
0
])

MU_GUILE_CHECK([
(display mu-package)(newline)
(display mu-version)(newline)
(display mu-package-string)(newline)
(display mu-bugreport)(newline)
(display mu-debug)(newline)
],
[0],[expout])
AT_CLEANUP
