AT_SETUP([mu-mime?])
AT_KEYWORDS([mime])
WITH_MAILBOX([spool/mime.msg],
 [MU_GUILE_CHECK([(exit (if (mu-mime? (mu-mime-create 0 (file->message "mbox"))) 0 1))])])
AT_CLEANUP   
