AT_SETUP([mu-mailbox-get-url])
AT_KEYWORDS([mailbox])
WITH_MAILBOX([spool/mbox1],
[MU_GUILE_CHECK([
(use-modules ((ice-9 regex)))
	     
(let ((s (mu-mailbox-get-url (mu-mailbox-open "mbox" "r"))))
  (cond
   ((string-match "^(.*/)?mbox$" s)
    (exit 0))
   (else
    (display s)
    (newline)
    (exit 1))))])])

AT_CLEANUP
 
