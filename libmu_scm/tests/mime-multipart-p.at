AT_SETUP([mu-mime-multipart?])
AT_KEYWORDS([mime])
WITH_MAILBOX([spool/mime.msg],
 [MU_GUILE_CHECK([
(display (mu-mime-multipart? (mu-mime-create 0 (file->message "mbox"))))
(display (mu-mime-multipart? (mu-mime-create 0 (test-message))))],
0,
[#t#f])])
AT_CLEANUP   
