AT_SETUP([message-get-header])
AT_KEYWORDS([message])
MU_GUILE_CHECK([
(let ((msg (test-message)))
  (display (mu-message-get-header msg "From"))
  (newline)
  (display (mu-message-get-header msg "Nonexistent"))
  (newline))],
0,
  [user@example.org
#f
])
AT_CLEANUP
