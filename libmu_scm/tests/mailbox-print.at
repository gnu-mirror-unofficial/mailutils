AT_SETUP([mailbox print])
AT_KEYWORDS([mailbox])
WITH_MAILBOX([spool/mbox1],
 [MU_GUILE_CHECK([
(use-modules ((ice-9 regex)))
	     
(let ((s (with-output-to-string
	   (lambda ()
	     (display (mu-mailbox-open "mbox" "r"))))))
  (cond
   ((string-match
       "^(#<mailbox[[:space:]]+).*/(mbox[[:space:]]+\\(5\\)>)$" s)
    (exit 0))
   (else
    (display s)(newline)
    (exit 1))))
  (newline)])])

AT_CLEANUP
