AT_SETUP([mu-mime-create (from file)])
AT_KEYWORDS([mime])
WITH_MAILBOX([spool/mime.msg],
 [MU_GUILE_CHECK([
(let ((msg (file->message "mbox")))
  (let ((mime (mu-mime-create 0 msg)))
    (display mime)))],
   0,
   [#<mime 2>])])
AT_CLEANUP   
